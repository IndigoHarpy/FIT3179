{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/IndigoHarpy/FIT3179/main/Sex_Participation.csv"
  },
  "width": "container",
  "mark": {
    "type": "line",
    "interpolate": "monotone"
  },
  "encoding": {
    "x": {
      "field": "Year",
      "timeUnit": "year",
      "title":"Year"
    },
    "y": {
      "field": "Participants",
      "type": "quantitative",
      "title": "Total Participants"
    },
    "color": {
      "field": "Sex",
      "title": "Sex",
      "legend":{"orient": "bottom"}
    }
  },
  "config": {}
}