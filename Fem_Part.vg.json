{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Female Participation in Soccer (2023)",
  "width": 800,
  "height": 400,
  "projection": {"type": "albers"},
  "data": {
    "url": "https://raw.githubusercontent.com/IndigoHarpy/FIT3179/main/Australia_states.json",
    "format": {"type": "topojson", "feature": "STE_2016_AUST"}
  },
  "transform": [
    {
      "lookup": "properties.STE_NAME16",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/IndigoHarpy/FIT3179/main/Australia_Soccer_Participation.csv"
        },
        "key": "State",
        "fields": ["Relative Female Participation"]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Relative Female Participation",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"},
      {"field": "Relative Female Participation", "type": "quantitative"}
    ]
  }
}